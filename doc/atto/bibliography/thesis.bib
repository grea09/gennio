  tags = {SFB-TRR-62,Planning,AutomatedReasoning},
  web_url = {https://www.uni-ulm.de/fileadmin/website<sub>u</sub>ni<sub>u</sub>lm/iui.inst.090/Publikationen/2016/Bercher16HybridPlanningComplexitiesSlides.pdf},
