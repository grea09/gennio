  author = {Sanin, Cesar and Toro, Carlos and Haoxi, Zhang and Sanchez, Eider and Szczerbicki, Edward and Carrasco, Eduardo and Peng, Wang and Mancilla-Amaya, Leonardo},
  date = {2012-07-01},
